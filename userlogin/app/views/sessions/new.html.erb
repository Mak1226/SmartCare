<p id='alert'><%= alert %></p>
<h1>Login</h1>
<%= form_tag sessions_path do |form| %>
 <div class='field'>
   <%= label_tag :email %>
   <br>
   <%= text_field_tag :email %>
 </div>
 <br>
 <div class='field'>
   <%= label_tag :password %>
   <br>
   <%= password_field_tag :password %>
 </div>
 <div class='actions'>
   <%= submit_tag 'Login' %>
 </div>
<% end %>
<br>
<%= link_to "Home", home_index_path %>